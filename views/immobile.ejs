<!DOCTYPE html>
<html lang="en" dir="ltr">

</html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>
        <%=newImmobiles[0].immobileType %>-<%=newImmobiles[0].tipology %>-<%=newImmobiles[0].county %>-<%=newImmobiles[0].parish %>
    </title>
    <!-- Links -->

    <link rel="stylesheet" href="/css/immobile.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/css/bootstrap.min.css"
        integrity="sha256-YLGeXaapI0/5IgZopewRJcFXomhRMlYYjugPLSyNjTY=" crossorigin="anonymous" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.9.0/css/all.min.css"
        integrity="sha256-UzFD2WYH2U1dQpKDjjZK72VtPeWP50NoJjd26rnAdUI=" crossorigin="anonymous" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.slim.js"
        integrity="sha256-BTlTdQO9/fascB1drekrDVkaKd9PkwBymMlHOiG+qLI=" crossorigin="anonymous"></script>
    <!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.15.0/esm/popper.min.js" integrity="sha256-3Iu0zFU6cPS92RSC3Pe4DBwjIV/9XKyzYTqKZzly6A8=" crossorigin="anonymous"></script> -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.3.1/js/bootstrap.min.js"
        integrity="sha256-CjSoeELFOcH0/uxWu6mC/Vlrc1AARqbm/jiiImDGV3s=" crossorigin="anonymous"></script>
</head>

<body>
    <% newImmobiles.forEach(function(immobile){ %>
    <!--Carousel Wrapper-->
    <div id="carousel-thumb" class="carousel slide carousel-fade carousel-thumbnails" data-ride="carousel">
        <!--Slides-->
        <div class="carousel-inner" role="listbox">
            <!-- /propertyFeatures/:id/:numberOfBedrooms/:rooms/:price/ -->
            <a href="/propertyFeatures/<%=newImmobiles[0]._id %>/<%=newImmobiles[0].immobileType %>/T<%=newImmobiles[0].numberOfBedrooms %>/<%=newImmobiles[0].price %>/"><i class="far fa-window-close"></i></a>
            <div class="carousel-item active">
                <% if(immobile.images[0].filename){  %>
                <img class="d-block w-100 center-block" data-slide-to="<%=immobile.images[0].filename%>"
                    src="/src/images/uploads/<%=immobile.images[0].filename %>"
                    alt="<%=immobile.immobileType %>-<%=immobile.tipology %>-<%=immobile.county %>-<%=immobile.parish %>-<%=immobile.images.length %>" ">
                    <%  } else{  %>
                        <img class="d-block w-100 center-block" data-slide-to="<%=immobile.images[0]%>"
                    src="/src/images/uploads/<%=immobile.images[0]%>"
                    alt="<%=immobile.immobileType %>-<%=immobile.tipology %>-<%=immobile.county %>-<%=immobile.parish %>-<%=immobile.images.length %>" ">
                    <% }  %>                    
            </div>
                
            <%  for (var i = 0; i < immobile.images.length-1; i++) { %>
                <div class=" carousel-item">
                <% if(immobile.images[i+1].filename){  %>
                <img class="d-block w-100" src="/src/images/uploads/<%=immobile.images[i+1].filename %>"
                    alt="<%=immobile.immobileType %>-<%=immobile.tipology %>-<%=immobile.county %>-<%=immobile.parish %>-<%=immobile.images.length %>" ">
                    <%  } else{  %>
                        <img class=" d-block w-100"src="/src/images/uploads/<%=immobile.images[i+1] %>"
                    alt="<%=immobile.immobileType %>-<%=immobile.tipology %>-<%=immobile.county %>-<%=immobile.parish %>-<%=immobile.images.length %>" ">
                        <% }  %>
            </div>
                <%  } %>
        </div>
        <!--/.Slides-->
        <!--Controls-->
        <a class=" carousel-control-prev" href="#carousel-thumb" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carousel-thumb" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
                <!--/.Controls-->
                <!-- Indicators -->
                <ol class="carousel-indicators" id="yscroll">
                    <% if(immobile.images[0].filename){  %>
                    <li data-target="#carousel-thumb" data-slide-to="<%=0%>" class="active">
                        <img src="/src/images/uploads/<%=immobile.images[0].filename %>" width="100">
                    </li>
                    <%  } else{  %>
                    <li data-target="#carousel-thumb" data-slide-to="<%=0%>" class="active">
                        <img src="/src/images/uploads/<%=immobile.images[0] %>" width="100">
                    </li>
                    <%  } %>
                    <%  for (var i = 0; i < immobile.images.length-1; i++) { %>
                    <% if(immobile.images[i+1].filename){  %>
                    <li data-target="#carousel-thumb" data-slide-to="<%=i+1%>">
                        <img src="/src/images/uploads/<%=immobile.images[i+1].filename %>" width="100">
                    </li>
                    <%  } else{  %>
                    <li data-target="#carousel-thumb" data-slide-to="<%=i+1%>">
                        <img src="/src/images/uploads/<%=immobile.images[i+1] %>" width="100">
                    </li>
                    <%  } %>
                    <%  } %>
                </ol>
                <script>
                    var pos = 0;
                    $(window).bind('mousewheel DOMMouseScroll', function (event) {
                        if (event.originalEvent.wheelDelta > 0 || event.originalEvent.detail < 0) {
                            pos = pos + 50;
                        } else {
                            if (pos > 1) {

                                pos = pos - 50;
                            }
                        }
                        $('#yscroll').scrollLeft(pos)
                    });
                </script>
            </div>
            <!--/.Carousel Wrapper-->
            <% }); %>
            <script src="/js/index.js"></script>
</body>